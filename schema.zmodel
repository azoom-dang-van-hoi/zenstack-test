import "zmodel/user"
import "zmodel/organization"
import "zmodel/parking"
import "zmodel/region"

generator client {
    provider        = "prisma-client-js"
    previewFeatures = ["interactiveTransactions"]
}

datasource db {
    provider = "mysql"
    url      = env("DATABASE_URL")
}

plugin myplugin {
    provider = './dist'
}

